{
  "homepage": "https://teumik.github.io/teumik-REACT2023Q2/module01/",
  "name": "react-2023q1",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "init": "npm install",
    "dev": "nodemon -x ts-node-esm server/index.ts",
    "lint": "npx eslint --fix src && npx prettier -w src",
    "test:coverage": "vitest run --coverage",
    "prebuild": "npm run lint && tsc && npm run test:coverage",
    "build:client": "npm run prebuild && vite build --outDir dist/client",
    "build:server": "npm run prebuild && vite build --ssr server/index.ts --outDir dist/server",
    "build": "npm run build:server & npm run build:client",
    "netlify:build": "npm run lint && tsc && vite build --outDir dist/client & vite build --ssr server/index.ts --outDir dist/server",
    "check-packages": "npx npm-check-updates",
    "update-packages": "npm run check:packages -u && npm run init"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.1.0",
    "@reduxjs/toolkit": "^1.9.5",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "react-redux": "^8.0.5",
    "react-router": "^6.10.0",
    "react-router-dom": "^6.10.0",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/express": "^4.17.17",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/coverage-c8": "^0.30.1",
    "eslint": "^8.39.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jsdom": "^21.1.1",
    "msw": "^1.2.1",
    "nodemon": "^2.0.22",
    "postcss-load-config": "^4.0.1",
    "prettier": "^2.8.8",
    "sass": "^1.62.0",
    "typescript": "^5.0.4",
    "vite": "^4.3.1",
    "vite-plugin-checker": "^0.5.6",
    "vitest": "^0.30.1"
  }
}
